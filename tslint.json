{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-microsoft-contrib",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "strict-boolean-expressions": [true, "allow-string", "allow-mix"],
    "no-unsafe-any": false,
    "no-implicit-dependencies": false,
    "no-submodule-imports": [true, "api", "interfaces", "values"]
  },
  "rulesDirectory": []
}
