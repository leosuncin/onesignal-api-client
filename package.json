{
  "name": "onesignal-api-client",
  "version": "0.0.0-development",
  "description": "Unofficial OneSignal API client for Node.js",
  "main": "dist/lib/Client.js",
  "types": "dist/lib/Client.d.ts",
  "typings": "dist/lib/Client.d.ts",
  "scripts": {
    "build": "tsc --pretty",
    "cz": "git-cz",
    "commitmsg": "commitlint -e",
    "lint": "tslint -p . -t stylish",
    "postmerge": "npm install",
    "prebuild": "rimraf dist",
    "precommit": "lint-staged",
    "prepublishOnly": "rollup -c rollup.config.js",
    "prepush": "npm run lint && npm test",
    "prettier": "prettier --write",
    "semantic-release":
      "semantic-release pre && npm publish && semantic-release post",
    "test": "nyc mocha"
  },
  "files": ["dist/", "README.md", "LICENSE"],
  "repository": {
    "type": "git",
    "url": "https://github.com/leosuncin/onesignal-api-client.git"
  },
  "keywords": [
    "OneSignal",
    "API",
    "Notifications",
    "Client",
    "Promise",
    "REST"
  ],
  "author":
    "Jaime Leonardo Suncin Cruz <leosuncin@gmail.com> (https://leosuncin.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leosuncin/onesignal-api-client/issues"
  },
  "homepage": "https://github.com/leosuncin/onesignal-api-client#readme",
  "dependencies": {
    "@types/got": "^9.6.9",
    "@types/node": "^13.1.7",
    "got": "^10.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/chai": "^4.0.3",
    "@types/mocha": "^5.0.0",
    "@types/nock": "^11.1.0",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^7.5.1",
    "chai": "^4.1.1",
    "commitizen": "^4.0.3",
    "custom-tslint-formatters": "^2.1.1",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "mocha": "^7.0.0",
    "nock": "^11.7.2",
    "nyc": "^15.0.0",
    "prettier": "^1.5.3",
    "proxyquire": "^2.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.29.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "semantic-release": "^16.0.2",
    "sinon": "^8.1.0",
    "source-map-support": "^0.5.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.0.0",
    "tslint": "^5.6.0",
    "tslint-config-prettier": "^1.3.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.7.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
