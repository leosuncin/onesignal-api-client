{
  "name": "onesignal-api-client",
  "version": "0.2.0",
  "description": "Unofficial OneSignal API client for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --pretty",
    "lint": "tslint -p . --type-check -t stylish",
    "postmerge": "npm install",
    "prebuild": "rimraf dist",
    "precommit": "lint-staged",
    "prepublishOnly": "rollup -c rollup.config.js",
    "prepush": "npm run lint && npm test",
    "prettier": "prettier --write",
    "test": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leosuncin/onesignal-api-client.git"
  },
  "keywords": [
    "OneSignal",
    "API",
    "Notifications",
    "Client",
    "Promise",
    "REST"
  ],
  "author": "Jaime Leonardo Suncin Cruz <leosuncin@gmail.com> (https://leosuncin.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leosuncin/onesignal-api-client/issues"
  },
  "homepage": "https://github.com/leosuncin/onesignal-api-client#readme",
  "dependencies": {
    "@types/got": "^7.1.1",
    "@types/node": "^8.0.24",
    "got": "^7.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.3",
    "@types/mocha": "^2.2.41",
    "@types/nock": "^8.2.1",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^2.3.3",
    "chai": "^4.1.1",
    "custom-tslint-formatters": "^2.1.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3",
    "mocha": "^3.5.0",
    "nock": "^9.0.14",
    "nyc": "^11.1.0",
    "prettier": "^1.5.3",
    "proxyquire": "^1.8.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.49.2",
    "rollup-plugin-typescript2": "^0.5.2",
    "sinon": "^3.2.1",
    "source-map-support": "^0.4.16",
    "ts-node": "^3.3.0",
    "tsconfig-paths": "^2.2.0",
    "tslint": "^5.6.0",
    "tslint-config-prettier": "^1.3.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.4.2"
  }
}
